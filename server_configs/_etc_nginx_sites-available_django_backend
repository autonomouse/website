server {
    # listen for ipv4; this line is default and implied:
    listen 80;

    # listen for ipv6
    listen [::]:80;

    # Make site accessible from http://api.localhost/
    server_name api.localhost;

    location /static/ {
        root unix:/var/www/django_backend/static;
    }

    location / {
        include proxy_params;
        proxy_pass http://unix:/var/www/django_backend/django_backend.sock;
    }

}
